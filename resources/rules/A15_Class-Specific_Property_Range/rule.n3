{
?constraint a rdfcv:SimpleConstraint ;
  :constraintType :ClassSpecificPropertyRange ;
  rdfcv:constrainingElement :property-range ;
  rdfcv:contextClass  ?c1 ;
  rdfcv:leftProperties ?p ;
  rdfcv:classes ?c .
?x ?p ?o.
?x a ?c1.
_:x e:findall (1 {?o a ?c} ()).

}
=>
{
_:x a :constraintViolation;
    :violatedConstraint ?constraint;
    :subject ?x;
    :property ?p;
    :object ?o;
    :expectedRange ?c.
}.



